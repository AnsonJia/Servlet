<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/j2ee/dtds/web-app_2_3.dtd">
<web-app id="WebApp_ID">
	<display-name>AnsonServletProject1</display-name>
	
	<filter>
   		<filter-name>ServletFilter</filter-name>
   		<filter-class>com.myservlet.ServletFilter</filter-class>
   		<init-param>
      		<param-name>test-param</param-name>
      		<param-value>Initialization Paramter</param-value>
   		</init-param>
		</filter>

	<filter-mapping>
   		<filter-name>ServletFilter</filter-name>
   		<url-pattern>/*</url-pattern>
	</filter-mapping>
	
	<servlet>
		<servlet-name>MyServlet1</servlet-name>
		<display-name>MyServlet1</display-name>
		<description></description>
		<servlet-class>com.myservlet.MyServlet1</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>HelloWorld</servlet-name>
		<display-name>HelloWorld</display-name>
		<description></description>
		<servlet-class>com.myservlet.HelloWorld</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FormData</servlet-name>
		<display-name>FormData</display-name>
		<description></description>
		<servlet-class>com.myservlet.FormData</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FormData2</servlet-name>
		<display-name>FormData2</display-name>
		<description></description>
		<servlet-class>com.myservlet.FormData2</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FormData3</servlet-name>
		<display-name>FormData3</display-name>
		<description></description>
		<servlet-class>com.myservlet.FormData3</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ClientRequest</servlet-name>
		<display-name>ClientRequest</display-name>
		<description></description>
		<servlet-class>com.myservlet.ClientRequest</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ServerResponse</servlet-name>
		<display-name>ServerResponse</display-name>
		<description></description>
		<servlet-class>com.myservlet.ServerResponse</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>HttpCodes</servlet-name>
		<display-name>HttpCodes</display-name>
		<description></description>
		<servlet-class>com.myservlet.HttpCodes</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RegistrationServlet</servlet-name>
		<display-name>RegistrationServlet</display-name>
		<description></description>
		<servlet-class>com.myservlet2.RegistrationServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EmploymentServlet</servlet-name>
		<display-name>EmploymentServlet</display-name>
		<servlet-class>com.myservlet2.EmploymentServlet</servlet-class>
	</servlet>

	
	
	
	
	<servlet-mapping>
		<servlet-name>MyServlet1</servlet-name>
		<url-pattern>/MyServlet1</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HelloWorld</servlet-name>
		<url-pattern>/HelloWorld</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FormData</servlet-name>
		<url-pattern>/FormData</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FormData2</servlet-name>
		<url-pattern>/FormData2</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FormData3</servlet-name>
		<url-pattern>/FormData3</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ClientRequest</servlet-name>
		<url-pattern>/ClientRequest</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServerResponse</servlet-name>
		<url-pattern>/ServerResponse</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HttpCodes</servlet-name>
		<url-pattern>/HttpCodes</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>RegistrationServlet</servlet-name>
		<url-pattern>/RegistrationServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EmploymentServlet</servlet-name>
		<url-pattern>/EmploymentServlet</url-pattern>
	</servlet-mapping>

	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
		
	</welcome-file-list>
</web-app>



